<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Phone_S</fullName>
    <active>true</active>
    <description>The phone should start from 6 o 7</description>
    <errorConditionFormula>NOT REGEX( Phone , &quot;([6|7]{1}[0-9]{8})&quot;)</errorConditionFormula>
    <errorDisplayField>MobilePhone</errorDisplayField>
    <errorMessage>The phone should start from 6 o 7</errorMessage>
</ValidationRule>
